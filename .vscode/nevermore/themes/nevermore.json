{
  "$schema": "vscode://schemas/color-theme",
  "name": "Nevermore",
  "type": "dark",
  "colors": {
    "titleBar.activeBackground": "#111111",
    "editor.background": "#111111",
    "editor.foreground": "#bbbbbb",
    "editorCursor.foreground": "#bbbbbb",
    "editorCursor.background": "#111111",
    "editor.lineHighlightBackground": "#111111",
    "editorLineNumber.foreground": "#555555",
    "editorLineNumber.activeForeground": "#bbbbbb",
    "editor.selectionBackground": "#313131",
    "editor.inactiveSelectionBackground": "#1a1a1a80",
    "editor.findMatchBackground": "#1a2d5f",
    "editor.findMatchHighlightBackground": "#1a2d5f80",
    "editorWidget.background": "#000000",
    "editorWidget.border": "#555555",
    "editorSuggestWidget.background": "#000000",
    "editorSuggestWidget.border": "#555555",
    "editorSuggestWidget.selectedBackground": "#222222",
    "statusBar.background": "#1F1F1F",
    "statusBar.foreground": "#ffffff",
    "statusBar.noFolderBackground": "#1F1F1F",
    "statusBar.debuggingBackground": "#1F1F1F",
    "statusBarItem.errorForeground": "#f7768e",
    "statusBarItem.warningForeground": "#ff9e64",
    "errorForeground": "#f7768e",
    "activityBar.background": "#111111",
    "activityBar.foreground": "#bbbbbb",
    "activityBar.inactiveForeground": "#555555",
    "sideBar.background": "#111111",
    "sideBar.foreground": "#bbbbbb",
    "sideBarTitle.foreground": "#111111",
    "sideBarSectionHeader.background": "#111111",
    "sideBarSectionHeader.foreground": "#bbbbbb",
    "list.activeSelectionBackground": "#222222",
    "list.activeSelectionForeground": "#bbbbbb",
    "list.inactiveSelectionBackground": "#1a1a1a",
    "list.hoverBackground": "#111111",
    "editorGroup.border": "#111111",
    "editorGroupHeader.tabsBackground": "#111111",
    "tab.activeBackground": "#101010",
    "tab.activeForeground": "#bbbbbb",
    "tab.inactiveBackground": "#1a1a1a",
    "tab.inactiveForeground": "#555555",
    "tab.border": "#111111",
    "panel.background": "#111111",
    "panel.border": "#111111",
    "terminal.background": "#111111",
    "terminal.foreground": "#bbbbbb",
    "terminal.ansiBlack": "#111111",
    "terminal.ansiRed": "#f7768e",
    "terminal.ansiGreen": "#009966",
    "terminal.ansiYellow": "#aaaa00",
    "terminal.ansiBlue": "#6666ee",
    "terminal.ansiMagenta": "#cc33cc",
    "terminal.ansiCyan": "#7dcfff",
    "terminal.ansiWhite": "#bbbbbb",
    "terminal.ansiBrightBlack": "#555555",
    "terminal.ansiBrightRed": "#f7768e",
    "terminal.ansiBrightGreen": "#009966",
    "terminal.ansiBrightYellow": "#aaaa00",
    "terminal.ansiBrightBlue": "#6666ee",
    "terminal.ansiBrightMagenta": "#cc33cc",
    "terminal.ansiBrightCyan": "#7dcfff",
    "terminal.ansiBrightWhite": "#ffffff",
    "editorError.foreground": "#7a3844",
    "editorWarning.foreground": "#ff9e64",
    "editorInfo.foreground": "#7dcfff",
    "editorHint.foreground": "#555555",
    "problemsErrorIcon.foreground": "#f7768e",
    "problemsWarningIcon.foreground": "#ff9e64",
    "problemsInfoIcon.foreground": "#7dcfff",
    "input.background": "#111111",
    "input.foreground": "#bbbbbb",
    "input.border": "#555555",
    "inputOption.activeBackground": "#222222",
    "button.background": "#7aa2f7",
    "button.foreground": "#111111",
    "button.hoverBackground": "#B8C8F7",
    "focusBorder": "#7aa2f7"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#555555",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#009966"
      }
    },
    {
      "name": "String Escape",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#7dcfff"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#9d7cd8"
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#ff9e64"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant.language", "constant.other"],
      "settings": {
        "foreground": "#9d7cd8"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.modifier", "storage.type"],
      "settings": {
        "foreground": "#7aa2f7"
      }
    },
    {
      "name": "Keyword Return",
      "scope": ["keyword.control.return", "keyword.control.flow"],
      "settings": {
        "foreground": "#9d7cd8"
      }
    },
    {
      "name": "Conditional & Repeat",
      "scope": ["keyword.control.conditional", "keyword.control.loop"],
      "settings": {
        "foreground": "#9d7cd8"
      }
    },
    {
      "name": "Type",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#7dcfff"
      }
    },
    {
      "name": "Built-in Type",
      "scope": ["support.type.builtin", "support.type.primitive"],
      "settings": {
        "foreground": "#7dcfff"
      }
    },
    {
      "name": "Class, Type Definition",
      "scope": ["entity.name.type", "entity.name.class"],
      "settings": {
        "foreground": "#bbbbbb"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#B8C8F7"
      }
    },
    {
      "name": "Function Call",
      "scope": ["meta.function-call", "variable.function"],
      "settings": {
        "foreground": "#B8C8F7"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#bbbbbb"
      }
    },
    {
      "name": "Variable Built-in",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#bbbbbb"
      }
    },
    {
      "name": "Property",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#aaaaaa"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#bbbbbb"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "meta.brace", "meta.delimiter"],
      "settings": {
        "foreground": "#bbbbbb"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "support.class.component"],
      "settings": {
        "foreground": "#B8C8F7"
      }
    },
    {
      "name": "Markup Heading",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#7aa2f7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
}
